{
    "name": "slm/queue-doctrine",
    "description": "Zend Framework module that integrates Doctrine as queuing system",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "zf2",
        "zf3",
        "expressive",
        "queue",
        "job",
        "doctrine",
        "db",
        "database"
    ],
    "homepage": "https://github.com/juriansluiman/SlmQueueDoctrine",
    "authors": [
        {
            "name": "Stefan Kleff",
            "email": "s.kleff@goalio.de",
            "homepage": "http://www.goalio.de"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "zendframework/zend-eventmanager": "^2.6 || ^3.0",
        "zendframework/zend-servicemanager": "^2.7 || ^3.0",
        "zendframework/zend-stdlib": "^2.2 || ^3.0",
        "slm/queue": "^1.0",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "zendframework/zend-config": "^2.2",
        "zendframework/zend-modulemanager": "^2.5",
        "zendframework/zend-view": "^2.5",
        "zendframework/zend-log": "^2.5",
        "zendframework/zend-i18n": "^2.5",
        "zendframework/zend-serializer": "^2.5",
        "zendframework/zend-mvc": "^2.5",
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "suggest": {
        "doctrine/doctrine-orm-module": "^2.0 to use DoctrineORMModule",
        "dasprid/container-interop-doctrine": "^1.1 to use dasprid/container-interop-doctrine implementation"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        },
        "zf": {
            "module": "SlmQueueDoctrine\\Module",
            "config-provider": "SlmQueueDoctrine\\ConfigProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "SlmQueueDoctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlmQueueDoctrineTest\\": "tests/"
        }
    }
}
